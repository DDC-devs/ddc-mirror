{-# OPTIONS -no-implicit-prelude #-}
import Data.Int
import Data.Int32U
import Data.String

foreign import "_ddcArgCount"
 argCountU :: Int32#
 
argCount = boxInt32 argCountU
 
 
foreign import "primArgValue"
 getArgValueU :: Int32# -> String#

