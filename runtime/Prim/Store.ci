#ifndef _DDC_Prim_Store_h
#define _DDC_Prim_Store_h

// -- Primitive store functions imported via DDC.Store

static inline Int32 primStore_peekDataR_size (Obj* obj)
{
	DataR *data	= (DataR*) obj;
	return (Int32)data->size;
}


static inline void* primStore_peekDataR_payload (Obj* obj)
{
	DataR* data	= (DataR*) obj;
	void*  payload	= (void*)data->payload;
	return	payload;
}


static inline void primStore_pokeInt32 (Int32* ptr, Int32 val)
{
	*ptr	= val;
}


#endif
