{-# OPTIONS -no-implicit-prelude #-}
-- See if CAFs work, including their initialisation code.
import Base
import Data.Numeric.Int32

foreign import "printf"
 printf :: String#{read} -> Int#

one	= 1
zero	= 0

countDown n
 = if n == zero
	then puts "Done!\n"#
	else countDown (n - one)

main ()
 = do	countDown 100
	()
