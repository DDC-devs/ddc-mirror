
main ()
 = do {	
 
 	strApp	= "Some " % "appended " % "string.\n";
 	print	strApp;
	
	print	$ "length = " % show strApp.length % "\n";
 
 
 	print	$ string_join "Joined " "string.\n";
	
	strFlat
		= string_flatten $ "Some " % "merged " % "string.\n";

	print	$ strFlat;
	print	$ "strMerged isFlat  = " % show (string_isFlat strFlat) % "\n";

	print	$ (show $ "Some appended string.\n" == strApp.flatten) % "\n";
	

 };
