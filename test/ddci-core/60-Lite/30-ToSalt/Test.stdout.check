-- Unboxed literal patterns.
module Main with
letrec {
  foo : Word16# -> Word16#
    = \(x : Word16#).
      case x of {
        5w16#  
         -> 6w16#;
        7w16#  
         -> 8w16#;
        _ 
         -> 0w16#
      }
}


