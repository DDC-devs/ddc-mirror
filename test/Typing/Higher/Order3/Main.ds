
-- Test out handling of a third order function.
--	At third order we can start to get ':>' constraints on effects.

succ :: Int -> Int;
succ x = x + 1;

danio g	= g 5;

-- perch is a third order function
perch f = succ (f succ);

appDanio f = f danio;

main ()
 = do {
	println $ show (perch danio);
	println $ show (appDanio perch);
 };




