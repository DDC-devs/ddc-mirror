

succ :: Int %r1 -> Int %r1;
succ x = x + 1;

succP :: Int -> Int;
succP x 
 = do {	n	= x + 1;
	print (showInt n);
	n;
 };


danio g	= g 5;

perch f = succ (f succ);

perchP f 
 = do {	n	= perch f;
	print (showInt n);
	n;
 };

appDanio f = f danio;

