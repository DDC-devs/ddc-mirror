

intMutable 	= 5;
intConst	= 5;

main ()
 = do {
	-- force intMutable to be mutable
 	intMutable	:= 27;
	
	-- force intConst to be const
	junk		= (+) @ intConst 27;
	
	
	-- return object cannot be both const and mutable at the same time
	result		= select True intMutable intConst;
	
 };
 
select b x y
 = case b of {
 	True	-> x;
	False	-> y;
 };
 
