

-- A module with no top-level things.
:parse..
module Test where;;


-- Parse module with some top-level things.
-- Top level things are signaled by 'where'.
:parse..
module Test where
x : Nat = 5
y : Nat = add# x x;;


-- Factorial function with primops.
:parse..
module Test where
fac (x : Nat) : Nat
 = case x of
        0       -> 1
        _       -> mul# 1 (fac (sub# x 1));;
