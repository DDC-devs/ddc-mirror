
module Test with
letrec {
  add : [a : Data].a -> a -> a
    = /\(a : Data).
       \(x y : a). add# [a] x y;
  
  length : [a : Data].List a -> Nat
    = /\(a : Data).
       \(xx : List a).
      case xx of {
        Nil  
         -> 0;
        Cons x xs 
         -> add [?] 1 (length [?] xs)
      }
}
