-- Fuse map into fold
module Test with
letrec {
  foldMap : [k : Rate].Series# k Int# -> Int#
    = /\(k : Rate).
       \(s : Series# k Int#).
      let x4__acc : Ref# Int# = new# [Int#] 0i# in
      let _ : Unit
            = loop# [k]
                  (\(^ : Int#).
                   let s__elem : Int# = next# [Int#] [k] s ^0 in
                   let x2__elem : Int#
                         = (\(x : Int#). mul# [Int#] x 2i#) s__elem in
                   let x1 : Int# = read# [Int#] x4__acc in
                   let _ : Unit
                         = write# [Int#] x4__acc (add# [Int#] x1 x2__elem) in
                   ()) in
      let x4 : Int# = read# [Int#] x4__acc in
      x4
}
