:set lang Flow


-- Single stream being eaten by multiple consumers.
:load..
module Test with letrec
test    [a : Data] [k : Rate]
        (elems : Stream k Int#)
        : Int#
 = do   
        sum     = fold# [:Int# Int#:] [k]
                        (add# [Int#]) 0i# elems

        prod    = fold# [:Int# Int#:] [k]
                        (mul# [Int#]) 1i# elems

        add# [:Int#:] sum prod;;

