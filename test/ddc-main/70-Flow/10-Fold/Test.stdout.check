-- Single stream being eaten by multiple consumers.
module Test with
letrec {
  test : [k : Rate].Stream k Int# -> Int#
    = /\(k : Rate).
       \(elems : Stream k Int#).
      let sum : Int#
            = fold# [k] [Int#] [Int#] (add# [Int#]) 0i# elems in
      let prod : Int#
            = fold# [k] [Int#] [Int#] (mul# [Int#]) 1i# elems in
      add# [Int#] sum prod
}
