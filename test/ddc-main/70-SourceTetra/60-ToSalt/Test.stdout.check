              _ 
               -> let ^ = allocSmall [rT] TAG0# 8# in
                  let ^ = payloadSmall [rT] ^0 in
                  let _
                        = poke# [rT] [Nat#] (castPtr# [rT] [Nat#] [Word8#] ^0) 0# 0# in
                  ^1
            };
        _ 
         -> let ^ = allocSmall [rT] TAG0# 8# in
            let ^ = payloadSmall [rT] ^0 in
            let _
                  = poke# [rT] [Nat#] (castPtr# [rT] [Nat#] [Word8#] ^0) 0# 0# in
