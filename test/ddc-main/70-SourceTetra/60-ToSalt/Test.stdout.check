    = λ(x : Ptr# rT Obj).
      let x1
            = let ^ = allocSmall [rT] TAG0# 8# in
              let ^ = payloadSmall [rT] ^0 in
              let _
                    = poke# [rT] [Nat#] (castPtr# [rT] [Nat#] [Word8#] ^0) 0# 3# in
