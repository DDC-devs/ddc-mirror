-- Flip an enumerated value.
module Test with
letrec {
  flipit : Ptr# rT Obj -> Ptr# rT Obj
    = λ(x : Ptr# rT Obj).
      case getTag [rT] x of {
        TAG0#  
         -> let ^ = allocBoxed [rT] TAG1# 0# in
            ^0;
        TAG1#  
         -> let ^ = allocBoxed [rT] TAG0# 0# in
            ^0;
        _ 
         -> fail# [Ptr# rT Obj]
      }
}


