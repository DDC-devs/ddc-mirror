              let ^ = payloadSmall [rT] ^0 in
              let ^
                    = peek# [rT] [Nat#] (castPtr# [rT] [Nat#] [Word8#] ^0) 0# in
              ^0 in
      case x0 of {
        0#  
         -> let x1
                  = let ^ = y in
                    let ^ = payloadSmall [rT] ^0 in
                    let ^
                          = peek# [rT] [Nat#] (castPtr# [rT] [Nat#] [Word8#] ^0) 0# in
                    ^0 in
            case x1 of {
              0#  
               -> let ^ = allocSmall [rT] TAG0# 8# in
                  let ^ = payloadSmall [rT] ^0 in
                  let _
                        = poke# [rT] [Nat#] (castPtr# [rT] [Nat#] [Word8#] ^0) 0# 1# in
