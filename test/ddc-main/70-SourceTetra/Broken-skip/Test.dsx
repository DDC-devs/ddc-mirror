

-- List map using the S computation type.
:to-core..
module Test where
data List (a : Data) where
        Nil     : List a
        Cons    : a -> List a -> List a

mapS    [a b : Data] [e : Effect]
        (f : a -> S e b) (xx : List a) : S e (List b)
 = case xx of
        Nil       -> box Nil
        Cons x xs -> Cons (f x) (run mapS f xs)
;;
