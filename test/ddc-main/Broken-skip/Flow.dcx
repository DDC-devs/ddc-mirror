



-- Multiply two series.
-- Combination of synth type arg and explicit type arg doesn't work.

:set Synth
:set TraceCheck
:flow-lower-kernel..
module Test with letrec
test    [k : Rate] (n  : RateNat# k)
        (v  : Vector# Float32#)
        (s1 : Series# k Float32#) (s2 : Series# k Float32#) 
        : Process#
 = do   
        s2      = smap2# (mul# [Float32#]) s1 s2

        sfill# [k] [Float32#] v s2
;;
