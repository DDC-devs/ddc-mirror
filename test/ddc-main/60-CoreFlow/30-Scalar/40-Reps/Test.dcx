:set lang Flow
:set Synth
:set SuppressLetTypes

-- TODO: We don't have lowering implemented for segmented ops yet.
:load..
module Test with letrec
repsme  [k1 : Rate]
        (lens : Series# k1 Nat#)
        (s1   : Series# k1 Float32#)
        (v    : Vector# Float32#)
        : Process#
 = smkSegd# lens
   (/\(k2 : Rate). \(segd : Segd# k1 k2). 
     let s2 = sreps# segd s1 in
     let s3 = smap#  (mul# 2.0f32#) s2 
     in  sfill# v s3)
;;


