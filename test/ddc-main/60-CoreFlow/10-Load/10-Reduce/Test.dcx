:set lang Flow
:set Synth


-- Single series being eaten by two consumers.
:load..
module Test with letrec
test    [k : Rate] (u1 u2 : Ref# Int#) 
        (elems : Series# k Int#) : Process#
 =      pjoin#  (sreduce# u1 (add# [Int#]) 0i# elems)
                (sreduce# u2 (mul# [Int#]) 1i# elems)
;;
