ok


-- Two possible schedules, but should be one with only one manifest array (ys)
module Test with
letrec {
  y$runproc$process : [ys$'$k : Rate].RateVec# ys$'$k Int# -> Ref# Int# -> [ys$'$k$PROC : Proc].Unit -> Process# ys$'$k$PROC ys$'$k
    = /\(ys$'$k : Rate).
       \(us$rv : RateVec# ys$'$k Int#).\(y$ref : Ref# Int#).
      /\(ys$'$k$PROC : Proc).
       \(_ : Unit).
      let us$s : Series# ys$'$k$PROC ys$'$k Int# = series# [ys$'$k$PROC] [ys$'$k] [Int#] us$rv in
      let y$proc : Process# ys$'$k$PROC ys$'$k
            = sreduce# [ys$'$k$PROC] [ys$'$k] [Int#] y$ref
                  (\(x2 x3 : Int#). add# [Int#] x2 x3) 0i# us$s in
      y$proc;
  
  xs$runproc$process : [ys$'$k : Rate].RateVec# ys$'$k Int# -> Vector# Int# -> Int# -> [ys$'$k$PROC : Proc].Unit -> Process# ys$'$k$PROC ys$'$k
    = /\(ys$'$k : Rate).
       \(us$rv : RateVec# ys$'$k Int#).\(ys : Vector# Int#).\(y : Int#).
      /\(ys$'$k$PROC : Proc).
       \(_ : Unit).
      let us$s : Series# ys$'$k$PROC ys$'$k Int# = series# [ys$'$k$PROC] [ys$'$k] [Int#] us$rv in
      let xs$s : Series# ys$'$k$PROC ys$'$k Int#
            = smap# [ys$'$k$PROC] [ys$'$k] [Int#] [Int#]
                  (\(x0 : Int#). add# [Int#] 1i# x0) us$s in
      let ys$s : Series# ys$'$k$PROC ys$'$k Int#
            = smap# [ys$'$k$PROC] [ys$'$k] [Int#] [Int#]
                  (\(x5 : Int#). add# [Int#] y x5) xs$s in
      let ys$proc : Process# ys$'$k$PROC ys$'$k = sfill# [ys$'$k$PROC] [ys$'$k] [Int#] ys ys$s in
      ys$proc;
  
  test : Vector# Int# -> Vector# Int#
    = \(us : Vector# Int#).
      let y$ref : Ref# Int# = new# [Int#] 0i# in
      let y$runproc : Unit
            = ratify1# [Int#] [Unit] us
                  (/\(ys$'$k : Rate).
                    \(us$rv : RateVec# ys$'$k Int#).
                   runProcess# [ys$'$k] (y$runproc$process [ys$'$k] us$rv y$ref)) in
      let y : Int# = read# [Int#] y$ref in
      let ys : Vector# Int#
            = vnew# [Int#] (vlength# [Int#] us) in
      let xs$runproc : Unit
            = ratify1# [Int#] [Unit] us
                  (/\(ys$'$k : Rate).
                    \(us$rv : RateVec# ys$'$k Int#).
                   runProcess# [ys$'$k] (xs$runproc$process [ys$'$k] us$rv ys y)) in
      ys
}


