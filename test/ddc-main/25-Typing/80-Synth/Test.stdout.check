
-- Check we can produce existential vars.
\(x : ?0). x
:*: ?0 -> ?0
:!: Pure
:$: Empty


-- Check order of existential vars.
\(x : ?0).\(y : ?2).\(z : ?4). y
:*: ?0 -> ?2 -> ?4 -> ?2
:!: Pure
:$: Empty


-- Function application.
\(f : ?0).\(x : ?2). f x
:*: (?2 -> ?3) -> ?2 -> ?3
:!: Pure
:$: Empty


-- Multiple use of argument.
\(f : ?0).\(x : ?2). f x x
:*: (?2 -> ?2 -> ?3) -> ?2 -> ?3
:!: Pure
:$: Empty


-- Higher order.
\(f : ?0).\(g : ?2).\(x : ?4).
f (g x)
:*: (?6 -> ?5) -> (?4 -> ?6) -> ?4 -> ?5
:!: Pure
:$: Empty
