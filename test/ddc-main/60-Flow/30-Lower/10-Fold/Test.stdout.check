-- Fuse map into fold
module Test with
letrec {
  foldMap : [k : Rate].Series# k Int# -> Int#
    = /\(k : Rate).
       \(s : Series# k Int#).
      let x4$acc : Ref# Int# = new# [Int#] 0i# in
      let _ : Unit
            = loop# [k]
                  (\(^ : Int#).
                   let s$elem : Int# = next# [Int#] [k] s ^0 in
                   let x2$elem : Int#
                         = (\(x0 : Int#).
                            (\(x : Int#). mul# [Int#] x 2i#) x0) s$elem in
                   let x1 : Int# = read# [Int#] x4$acc in
                   let _ : Unit
                         = write# [Int#] x4$acc
                               ((\(x3 _ : Int#). add# [Int#] x1 x3) x2$elem ^0) in
                   ()) in
      let x4 : Int# = read# [Int#] x4$acc in
      x4
}
