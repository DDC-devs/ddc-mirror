-- Nested filter.
-- Ensure we can handle nested selector contexts.
module Test with
letrec {
  filter : [k0 : Rate].[a : Data].(a -> Bool#) -> (a -> Bool#) -> Series# k0 a -> Vector# a
    = /\(k0 : Rate)./\(a : Data).
       \(f g : a -> Bool#).\(s0 : Series# k0 a).
      let k1$count = new# [Nat#] 0# in
      let k2$count = new# [Nat#] 0# in
      let x4 = vnewR# [a] [k0] in
      let _
            = loop# [k0]
                  (\(x0 : Nat#).
                   let s0$elem = next# [a] [k0] s0 x0 in
                   let x1$elem = f s0$elem in
                   let _
                         = guard# k1$count x1$elem
                               (\(x1 : Nat#).
                                let x3$elem = g s0$elem in
                                let _
                                      = guard# k2$count x3$elem
                                            (\(x2 : Nat#).
                                             let _ = vwrite# [a] x4 x2 s0$elem in
                                             ()) in
                                ()) in
                   ()) in
      let x3 = read# [Nat#] k2$count in
      let x4 = vslice# [a] x3 x4 in
      x4
}

-- Sequential filter.
-- Ensure we can handle two selector contexts on the same input.
module Test with
letrec {
  filter : [k0 : Rate].[a : Data].(a -> Bool#) -> (a -> Bool#) -> Series# k0 a -> Tuple2# (Vector# a) (Vector# a)
    = /\(k0 : Rate)./\(a : Data).
       \(f g : a -> Bool#).\(s0 : Series# k0 a).
      let k1$count = new# [Nat#] 0# in
      let k2$count = new# [Nat#] 0# in
      let x4 = vnewR# [a] [k0] in
      let x7 = vnewR# [a] [k0] in
      let _
            = loop# [k0]
                  (\(x0 : Nat#).
                   let s0$elem = next# [a] [k0] s0 x0 in
                   let x1$elem = f s0$elem in
                   let x3$elem = f s0$elem in
                   let _
                         = guard# k1$count x1$elem
                               (\(x1 : Nat#).
                                let _ = vwrite# [a] x4 x1 s0$elem in
                                ()) in
                   let _
                         = guard# k2$count x3$elem
                               (\(x2 : Nat#).
                                let _ = vwrite# [a] x7 x2 s0$elem in
                                ()) in
                   ()) in
      let x3 = read# [Nat#] k1$count in
      let x4 = vslice# [a] x3 x4 in
      let x5 = read# [Nat#] k2$count in
      let x7 = vslice# [a] x5 x7 in
      T2# [Vector# a] [Vector# a] x4 x7
}



