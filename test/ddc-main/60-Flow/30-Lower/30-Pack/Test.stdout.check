-- Pack with an existing series of flags.
module Test with
letrec {
  packMe : [k0 : Rate].[a : Data].Series# k0 a -> Series# k0 Bool# -> Vector# a
    = /\(k0 : Rate)./\(a : Data).
       \(s : Series# k0 a).\(flags : Series# k0 Bool#).
      let k1$count = new# [Nat#] 0# in
      let x0 = vnewR# [a] [k0] in
      let _
            = loop# [k0]
                  (\(x1 : Nat#).
                   let s$elem = next# [a] [k0] s x1 in
                   let flags$elem = next# [Bool#] [k0] flags x1 in
                   let _
                         = guard# k1$count flags$elem
                               (\(x3 : Nat#).
                                let _ = vwrite# [a] x0 x3 s$elem in
                                ()) in
                   ()) in
      let x4 = read# [Nat#] k1$count in
      let x0 = vslice# [a] x4 x0 in
      x0
}


