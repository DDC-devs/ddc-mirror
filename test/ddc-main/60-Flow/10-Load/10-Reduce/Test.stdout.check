-- Single series being eaten by two consumers.
module Test with
letrec {
  test : [k : Rate].Ref# Int# -> Ref# Int# -> Series# k Int# -> Process#
    = /\(k : Rate).
       \(u1 u2 : Ref# Int#).\(elems : Series# k Int#).
      pjoin#
          (sreduce# [k] [Int#] u1 (add# [Int#]) 0i# elems)
          (sreduce# [k] [Int#] u2 (mul# [Int#]) 1i# elems)
}
