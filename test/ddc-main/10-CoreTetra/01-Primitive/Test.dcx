:set lang Tetra

-- The Tetra fragment uses the pure function type.
:kind (->)

-- Base types.
:kind Bool#
:kind Nat#
:kind Int#
:kind Word8#
:kind Word16#
:kind Word32#
:kind Word64#

-- Compuation type.
:kind S

-- Effect types.
:kind Read
:kind Write
:kind Alloc

-- Primitive Arithmetic.
:type neg#
:type add#
:type sub#
:type mul#
:type div#
:type mod#
:type rem#
:type eq#
:type neq#
:type gt#
:type ge#
:type lt#
:type le#
:type and#
:type or#
:type shl#
:type shr#
:type band#
:type bor#
:type bxor#

-- Mutable References.
:type allocRef#
:type readRef#
:type writeRef#

