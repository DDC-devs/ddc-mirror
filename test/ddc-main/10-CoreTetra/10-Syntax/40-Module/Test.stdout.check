-- Load a module definition.
module Main 
export value main : Unit -> S Pure Unit;
export value double : Nat# -> Nat#;
import value two : Unit -> Nat#;
with
private rt with {w1 : Const rt; w2 : Lazy rt}
let one : Nat# = 1#
letrec {
  main : Unit -> S Pure Unit
    = \(_ : Unit).
      box
      private r0 in
      ();
  
  double : Nat# -> Nat#
    = \(x : Nat#).
      mul# [Nat#] x (two ());
  
  thing : [r : Region].Nat# -> Nat#
    = /\(r : Region).
       \(x : Nat#). add# [Nat#] x one
}


