
import Base;
import System.File;
import Data.Maybe;


main () = topHandle prog;

name	= "test/Library/File/Main.data";

prog ()
 = do {	print	$ "length = " % show (file_length name) % "\n";

	case file_read name of
	 Nothing 
	  -> do	"failed\n"
		()

	 Just ss
	  -> print	$ "data = " % ss % "\n"
 };
