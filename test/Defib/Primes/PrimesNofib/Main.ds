
-- Primes2
--	Find the 1500'th prime number.
--	Based on the NoFib example.
--

suCC x		= x + 1;
isDivs n x	= mod x n /= 0;

theFilter xx
 = case xx of {
 	Cons x xs 	
	 -> filterL (isDivs x) xs;
 };

primes ()	= mapL head (iterateL theFilter (iterateL suCC 2));

main ()
 = do {
	print	$ showInt $ (primes ()) !! 1500;
	print "\n";
 };
