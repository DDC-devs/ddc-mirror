

-- Error: conflicting witnesses
:check\
 letregion r1 with {w1 : Mutable r1; w2 : Const r1} in\
 ()

-- Error: conflicting witnesses
:check\
 letregion r1 with {w1 : Lazy r1;    w2 : Direct r1} in\
 ()

-- Error: wrong region in witness set
:check\
 letregion r1 with {w1 : Mutable r1} in\
 letregion r2 with {w1 : Const r1}   in\
 ()

-- Error: invalid witness type
:check\
 letregion r1 with {w1 : Use r1} in\
 ()

-- Error: mistyped witness
:check\
 letregion r1 with {w1 : Mutable r1 r1} in\
 ()
