main ()
 = do	putStr $ show (test1_1 0) 	% "\n"
	putStr $ show (test1_2 'a')	% "\n"
	putStr $ show (test1_3 4.5) 	% "\n"
--	putStr $ show (test1_4 "dude") 	% "\n"
--	putStr $ show (test1_5 "dude") 	% "\n"


	putStr $ show (test2_1 7)	% "\n"
	putStr $ show (test2_1 'z')	% "\n"
	putStr $ show (test2_1 10.0)	% "\n"
	
	