
-- boxed match
test1_1 x
 = case 0 of
	0	-> 5
	1	-> 6
 	_	-> 7

test1_2 x
 = case x of
	'a'	-> 5
 	'b'	-> 6
	_	-> 7

test1_3 x
 = case x of
	4.5 	-> 5
 	6.7 	-> 6
	_	-> 7
{-
