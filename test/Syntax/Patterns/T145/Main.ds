	"wibble" -> 6
	_	-> 7

test1_5 x
 = case x of	
	"dude"	-> 5
-}	


-- unboxed match
test2_1 x
 = case 0# of
	23#	-> 5
	42#	-> 6 
 	_ 	-> 7

test2_2 x
 = case x of
	'a'# 	-> 5
 	'b'# 	-> 6
	_	-> 7

test2_3 x
 = case x of
	4.5# 	-> 5
 	6.7# 	-> 6
	_	-> 7



