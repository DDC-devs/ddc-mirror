
module Main
import Codec.Json
import System.IO.Console
where

main ()
 = do   printl $ parseJson "null"

        printl $ parseJson "true"
        printl $ parseJson "false"

        printl $ parseJson "\"hello world\""

        printl $ parseJson "1234"
        printl $ parseJson "-1234"
        printl $ parseJson "1234567"
        printl $ parseJson "1234567891"
        printl $ parseJson "1234.567"
        printl $ parseJson "-1234.567"
        printl $ parseJson "10006.99430671"
        printl $ parseJson "123.4567e5"
        printl $ parseJson "123.4567e+5"
        printl $ parseJson "1.234567e-5"

        printl $ parseJson "[]"
        printl $ parseJson "[true]"
        printl $ parseJson " [ true , false , true ] "
        printl $ parseJson "[true,123456,true,\"derp\"]"

        printl $ parseJson "{}"

        printl $ parseJson "{ \"one\" : [true,  false,  27.123,true  ], \"two\" : {\"three\":null}}"
        printl $ parseJson "{\"one\":{\"three\":[true,false,27,true],\"two\":{\"three\":null}}}"

        printl $ parseJson "[[\"funding\",\"BTC\",0.1,null],[\"funding\",\"USD\",10000.001,0,null]]"