
module Main
import Class.Show
import Data.Maybe
import Data.Function
import Data.Numeric
import System.IO.Console
where


-- | The first guard is inexhaustive, and will cause a runtime
--   error if x /= 0.
derp (mx: Maybe Nat#): Nat#
 = case mx of
        Just x
         | x == 0       -> 10

        Nothing         -> 20


main (_: Unit): S Console Unit
 = box do
        writel $ showNat (derp (Just 0))
        writel $ showNat (derp (Just 1))