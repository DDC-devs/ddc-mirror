
-- Check handling of suspended partial applications.
--	There usually aren't any of these in the source code,
--	but they can be introduced by the full laziness transform.
--

add :: Int -> Int -> Int;
add x y		= x + y;

main ()
 = do {	
	add5	= suspend2 add 5;
 	print	$ (showInt (add5 27)) % "\n";
 }; 
