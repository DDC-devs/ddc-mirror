data Base.Int %r1 where
        KIND       = % -> *
        MATERIAL   = Just {%r1}
        IMMATERIAL = Just {}
        MkInt
                :: forall %r1
                .  Int %r1
                with { ARITY  = 0
                     , TAG    = 0
                     , FIELDS = {}
                     , TYPES  = [] }
        
data List %r1 a1 where
        KIND       = % -> * -> *
        MATERIAL   = Just {%r1, a1}
        IMMATERIAL = Just {}
        Nil
                :: forall %r1 a1
                .  List %r1 a1
                with { ARITY  = 0
                     , TAG    = 0
                     , FIELDS = {}
                     , TYPES  = [] }
        
        Cons
                :: forall %r1 a1
                .  a1 -> List %r1 a1
                with { ARITY  = 1
                     , TAG    = 1
                     , FIELDS = {}
                     , TYPES  = [] }
        
data IntFun %r1 %r2 %r3 %r4 !e1 $c1 where
        KIND       = % -> % -> % -> % -> ! -> $ -> *
        MATERIAL   = Just {%r1, $c1}
        IMMATERIAL = Just {%r3, %r4, !e1}
        SInt
                :: forall %r1 %r2 %r3 %r4 !e1 $c1
                .  Int %r1 -> IntFun %r1 %r2 %r3 %r4 !e1 $c1
                with { ARITY  = 1
                     , TAG    = 0
                     , FIELDS = {}
                     , TYPES  = [] }
        
        SFun
                :: forall %r1 %r2 %r3 %r4 !e1 $c1
                .  (Int %r3 -(!e1 $c1)> Int %r4) -> IntFun %r1 %r2 %r3 %r4 !e1 $c1
                with { ARITY  = 1
                     , TAG    = 1
                     , FIELDS = {}
                     , TYPES  = [] }
        
data FunOrder2 %r1 a1 a2 a3 $c1 !e1 $c2 !e2 where
        KIND       = % -> * -> * -> * -> $ -> ! -> $ -> ! -> *
        MATERIAL   = Just {%r1, $c2}
        IMMATERIAL = Just {a1, !e1, $c1, a2, a3, !e2}
        FunOrder2
                :: forall %r1 a1 a2 a3 $c1 !e1 $c2 !e2
                .  ((a1 -(!e1 $c1)> a2) -(!e2 $c2)> a3) -> FunOrder2 %r1 a1 a2 a3 $c1 !e1 $c2 !e2
                with { ARITY  = 1
                     , TAG    = 0
                     , FIELDS = {}
                     , TYPES  = [] }
        
