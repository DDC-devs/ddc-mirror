module Main 
export main
where


-- Ackermann's function.
ack (m : Nat#) (n : Nat#) : Nat#
 = case m of
        0       -> n + 1
        _       -> case n of
                        0       -> ack (m - 1) 1
                        _       -> ack (m - 1) (ack m (n - 1))


main (_ : Unit) : S Console Unit
 = private r in
   box do
        run putStrLn (showNat [r] (ack 3 2))
        ()

