module Main 
import Data.Numeric.Nat
import System.IO.Console
where

-- Compute the factorial of some number.
fac (acc : Nat#) (n : Nat#) : Nat#
 = case n of
        0       -> acc
        1       -> acc
        _       -> fac (acc * n) (n - 1)


main (_ : Unit) : S Console Unit
 = private r in
   box do
        run putStrLn (showNat [r] (fac 1 10))
        ()
        
