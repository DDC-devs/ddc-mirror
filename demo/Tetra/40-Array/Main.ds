
module Main
import Data.Numeric.Nat
import Data.Array
import System.IO.Console
where


main (_: Unit): S Console Unit
 = private r with { Alloc r; Read r; Write r } in
   do   
        arr     = allocArray [r] 10 0

        writeArray arr 5 100

        writel (showNat (readArray arr 5))
        ()
