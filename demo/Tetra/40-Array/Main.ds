
module Main
import Data.Numeric.Nat
import Data.Array
import System.IO.Console
where


main (_: Unit): S Console Unit
 = box private r with { Alloc r; Read r; Write r } in
   do   
        arr     = run allocArray [r] 10 0

        run writeArray arr 5 100

        run writel (showNat (run readArray arr 5))
