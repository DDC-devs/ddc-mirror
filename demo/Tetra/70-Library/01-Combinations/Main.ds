
module Main
import Math.Combinations
import System.IO.Console
import Data.Numeric.Nat
import Data.Function
where


main (_: Unit): S Console Unit
 = box do
        run writel ("factorial 10            = " % showNat (factorial 10))
        run writel ("choose 20 3             = " % showNat (choose 20 3))

        run writel $ "chooseMany 10 [5, 2, 3] = " 
                   % showNat (chooseMany 10 (Cons 5 (Cons 2 (Cons 3 Nil))))
