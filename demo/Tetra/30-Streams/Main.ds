
module Main 
import Data.Stream
import System.IO.Console
where

main (_ : Unit) : S Console Unit
 = box do   
        xx1     = enumFromTo 0 100
        ss1     = streamOfList xx1
        ss2     = stake 10 (smap (add 1) ss1)
        xx2     = listOfStream ss2
        run putStrLn (showNat (foldl add 0 xx2))
