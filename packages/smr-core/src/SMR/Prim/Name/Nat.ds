
module SMR.Prim.Name.Nat
export isPrimNameNat
import Data.Text
where


-- | Check if this is the name of a Nat primitive.
isPrimNameNat (tx: Text): Bool
 | tx == "nat-add"      = True
 | tx == "nat-sub"      = True
 | tx == "nat-mul"      = True
 | tx == "nat-div"      = True
 | tx == "nat-eq"       = True
 | tx == "nat-neq"      = True
 | tx == "nat-lt"       = True
 | tx == "nat-le"       = True
 | tx == "nat-gt"       = True
 | tx == "nat-ge"       = True
 | otherwise            = False 
