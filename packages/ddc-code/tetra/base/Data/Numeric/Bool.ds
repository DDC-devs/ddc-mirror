
module Data.Numeric.Bool
export  { not; and; or }
where


-- | Boolean NOT.
not (x : Bool#)   : Bool#
 = if x then False  
        else True


-- | Boolean AND.
and (x y : Bool#) : Bool#
 = if x then y 
        else False


-- | Boolean OR.
or (x y : Bool#) : Bool#
 = if x then True 
        else y


